<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zking.nacosprovider.mapper.DictionaryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zking.nacosprovider.model.Dictionary">
    <result column="id" property="id" />
        <result column="dictType" property="dictType" />
        <result column="dictItem" property="dictItem" />
        <result column="dictValue" property="dictValue" />
        <result column="dictIsEditable" property="dictIsEditable" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        dictType, dictItem, dictValue, dictIsEditable
    </sql>

</mapper>
